{
  "name": "khm/membership",
  "description": "KHM Membership plugin scaffold",
  "type": "wordpress-plugin",
  "require": {
    "php": ">=7.4",
    "stripe/stripe-php": "^10.0",
    "dompdf/dompdf": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "brain/monkey": "^2.6",
    "mockery/mockery": "^1.6",
    "squizlabs/php_codesniffer": "^3.9",
    "wp-coding-standards/wpcs": "^3.0",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "KHM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KHM\\Tests\\": "tests/"
    }
  },
  "bin": [
    "bin/migrate.php"
  ],
  "minimum-stability": "dev",
  "license": "MIT",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "scripts": {
    "lint": "phpcs -q",
    "lint:report": "phpcs --report=full"
  }
}
